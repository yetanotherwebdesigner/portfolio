    a{ font-weight:bold; }
}
p{ line-height:1.5em;margin:1.5em 0; }
.holder{ 
         h2{ margin:.5em;text-decoration:underline; }
         p{ margin:1em;padding:0 0 1em; }
         h2+p{ font-size:14px;font-size:1.4rem; }/* intro text */
}
