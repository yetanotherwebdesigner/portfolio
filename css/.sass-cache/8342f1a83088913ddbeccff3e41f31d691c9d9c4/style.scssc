/* HOME */
#home { width:80%;margin:0 auto;
        p{font-size:14px;font-size:1.4rem;text-align:center;}
}
#home #menu{ width:100%;left:0;padding:2em 0;}
#menu a:link,#menu a:visited{ position:relative;text-decoration:none;width:100px;padding:40px 0 0;margin:3px;display:inline-block;height:61px;color:#fff;font-weight:bold;background-repeat:no-repeat;background-image:url("http://cdn.yetanotherwebdesigner.com/images/m-nav-bg.png");text-align:center;}
#blog,#blog-small { background-position:0 0;}
#portfolio,#snapshots-small{ background-position:-200px 0;}
#contact,#contact-small{ background-position:-100px 0;}
#y-gplus{ background-position:-300px 0; }
.tooltip{text-indent:-9999px;background:transparent;}/* no tooltip for basic styling */
/* TWEETS */
#tweets,#snapshots,#message{ position:relative; }
.slideshow{ width:100%;margin:0;padding:0;position:relative;
            img { display:block;width:95%;margin:25px auto;padding:0;}
}
.polaroid { background:#fff;padding:20px;margin:10px 0;border-radius:50px;
&:last-child{ margin-bottom:20px; }
            }
.no-js .polaroid h2,.no-js .polaroid p,.no-js .polaroid .arrows{ display:none;}
.js .arrows{ display:none; }
.js .polaroid h2, .js .polaroid p{ display:block; }
.arrows .prev, .arrows .next{ font-size:1.25em;text-decoration:none;margin:0 20px;background:#4d4d4d;color:#fff;border-radius:10px;padding:5px; }
/* TWEETS */
.tweet .tweet_list{ margin:0;padding:0;list-style-type:none; }
.tweet .tweet_list li, .query .tweet_list li {margin:0 0 20px;padding:40px;background:#ff0066;color:#fff;line-height:200%;@include shadow;border-radius:50px; }
.tweet .tweet_list li a, .query .tweet_list li a{color:#fff;background:#550022;border-radius:50px;text-decoration:none;padding:5px;}
.scroll-pane{position:relative;height:auto;}
.no-js .scroll-pane{ min-height:150px; }
#twitter-home{ display:inline-block;margin-bottom:50px; }
/* CONTACT */
h1 label{ font-size:1.8rem;font-size:18px; }
.postcard p{ text-align:center; }
p.remain{ display:none; }
input,textarea{ width:100%;background:transparent;border:1px solid #4d4d4d;margin:20px 0 0;border-radius:5px;
                &:focus{ border:1px solid #ccff00; }
                }
textarea{ min-height:200px; }
input[type="submit"]{ background:#ccff00 url("http://cdn.yetanotherwebdesigner.com/images/loading-mobile.gif") no-repeat 1em 100px;border:none;color:#fff;padding:10px;border-radius:5px;font-weight:bold;@include shadow(); }
#cform .formerror span,#cform label.error{ background:#ff0000;color:#fff;font-size:12px;font-size:1.2rem;padding:5px;border-radius:5px; }
.thankyou p{ text-align:center; }
/* ERRORS,404 */
.js div.error{ display:none; }
.no-js div.error{ z-index:10;margin:0 auto;position:relative;}
h2.fourofour{ font-family:Lucida Sans Unicode,Lucida Grande,sans-serif; }
.no-js #snapshots+.copyright+div.error{ top:-250px; }
.no-js #tweets+.copyright+div.error{ top:-350px; }
