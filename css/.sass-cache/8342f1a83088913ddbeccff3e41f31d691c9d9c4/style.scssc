    a{ font-weight:bold; }
}
p{ line-height:1.5em;margin:1.5em 0; }
.holder{ 
         h2{ margin:.5em;text-decoration:underline; }
         p{ margin:1em;padding:0 0 1em; }
         h2+p{ font-size:14px;font-size:1.4rem; }/* intro text */
}
a{ 
 &:link,&:visited{ outline:none;text-decoration:underline;color:#4d4d4d; }
 &:hover,&:active{ color:#4d4d4d; }
 img{ border:none; }
}
.copyright{
            margin:2em 0;
            p{
                text-align:center;font-size:10px;font-size:1rem;color:#5c5c5c;
               }
